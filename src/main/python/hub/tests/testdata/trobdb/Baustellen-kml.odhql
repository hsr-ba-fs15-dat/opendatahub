SELECT t.projektleiter as userid,
       substring(t.name, 1, 100) as title,
       t.projektbeschrieb as description,
       to_char(to_date(t.baubeginn, '%d.%m.%Y'), '%Y-%m-%d') as trob_start,
       to_char(to_date(t.bauende, '%d.%m.%Y'), '%Y-%m-%d') as trob_end,
       null as trob_interval,
       'both' as direction,
       null as diversion_advice,
       'CH' as country,
       t.projektbeschrieb as reason,
       t.abschnitt as object_name,
       'street' as object_type,
       'closed' as trob_type,
       t.geometry as geometry
  from TRF2004 as t
